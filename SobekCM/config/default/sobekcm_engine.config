<?xml version="1.0" encoding="utf-8" ?>
<SobekCM_Config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://sobekrepository.org/schemas/sobekcm_config"
	xsi:schemaLocation="http://sobekrepository.org/schemas/sobekcm_config
		http://sobekrepository.org/schemas/sobekcm_config.xsd">
  <Engine ClearAll="true">
    <Mapping>
      <Path Segment="custom">
        <ComplexEndpoint Segment="register">
          <VerbMapping Verb="POST" ComponentID="X1" Method="Register_User" Protocol="JSON" />
        </ComplexEndpoint>
      </Path>
      
      
      
      <Path Segment="aggregations">
        <Path Segment="all">
          <AutoDoc>
            <Description>Gets a single aggregation object, fully built and language specific</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>none</Arguments>
            <Returns>List&lt;SobekCM.Core.Aggregations.Item_Aggregation_Related_Aggregations&gt;</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C1" Method="GetAllAggregations" RestrictionRangeID="R1" Protocol="JSON" />         
          <Endpoint Segment="json-p" ComponentID="C1" Method="GetAllAggregations" RestrictionRangeID="R1" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C1" Method="GetAllAggregations" RestrictionRangeID="R1" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C1" Method="GetAllAggregations" RestrictionRangeID="R1" Protocol="XML" />
        </Path>
        <Path Segment="childpage">
          <AutoDoc>
            <Description> Gets the all information, including the HTML, for an item aggregation child page </Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Aggregation code, language, and child page code </Arguments>
            <Returns>SobekCM.Core.WebContent.HTML_Based_Content</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C1" Method="GetCollectionStaticPage" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C1" Method="GetCollectionStaticPage" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C1" Method="GetCollectionStaticPage" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C1" Method="GetCollectionStaticPage" Protocol="XML" />
        </Path>
        <Path Segment="code">
          <AutoDoc>
            <Description>Gets a single aggregation object, fully built and language specific</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Aggregation code and language</Arguments>
            <Returns>SobekCM.Core.Aggregations.Item_Aggregation</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C1" Method="GetAggregationByCode" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C1" Method="GetAggregationByCode" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C1" Method="GetAggregationByCode" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C1" Method="GetAggregationByCode" Protocol="XML" />
        </Path>
        <Path Segment="complete">
          <AutoDoc>
            <Description>Gets a single aggregation object, fully built</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Aggregation code</Arguments>
            <Returns>SobekCM.Core.Aggregations.Complete_Item_Aggregation</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C1" Method="GetCompleteAggregationByCode" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C1" Method="GetCompleteAggregationByCode" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C1" Method="GetCompleteAggregationByCode" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C1" Method="GetCompleteAggregationByCode" Protocol="XML" />
        </Path>
        <Path Segment="hierarchy">
          <AutoDoc>
            <Description>Gets the complete aggregation hierarchy</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>none</Arguments>
            <Returns>SobekCM.Core.Aggregations.Aggregation_Hierarchy</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C1" Method="GetCollectionHierarchy" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C1" Method="GetCollectionHierarchy" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C1" Method="GetCollectionHierarchy" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C1" Method="GetCollectionHierarchy" Protocol="XML" />
        </Path>
        <Endpoint Segment="uploads" ComponentID="C1" Method="GetAggregationUploadedImages" Protocol="JSON">
          <AutoDoc>
            <Description>Gets the list of uploaded images for this aggregation (for consumption by web users)</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Aggregation code</Arguments>
            <Returns>List&lt;SobekCM.Engine_Library.JSON_Client_Helpers.UploadedFileFolderInfo&gt;</Returns>
          </AutoDoc>
        </Endpoint>
      </Path>
      <Path Segment="items">
        <Path Segment="brief">
          <AutoDoc>
            <Description>Gets the item information (primarily citation right now)</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>BibID and VID</Arguments>
            <Returns>BriefItemInfo</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C3" Method="GetItemBrief" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C3" Method="GetItemBrief" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C3" Method="GetItemBrief" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C3" Method="GetItemBrief" Protocol="XML" />
        </Path>
        <Path Segment="citation">
          <AutoDoc>
            <Description>Gets the item information (primarily citation right now)</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>BibID, and VID</Arguments>
            <Returns>BriefItemInfo</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C3" Method="GetItemCitation" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C3" Method="GetItemCitation" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C3" Method="GetItemCitation" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C3" Method="GetItemCitation" Protocol="XML" />
        </Path>
        <Path Segment="legacy">
          <Endpoint Segment="json" ComponentID="C3" Method="Get_Item_Info_Legacy" Protocol="JSON">
            <AutoDoc>
              <Description>Gets the search results and returns them in the legacy format supported prior to v5.0</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments></Arguments>
              <Returns>Legacy formatted JSON response</Returns>
            </AutoDoc>
          </Endpoint>
        </Path>
        <Endpoint Segment="random" ComponentID="C3" Method="GetRandomItem" Protocol="JSON-P">
          <AutoDoc>
            <Description>Gets a random item from this instance</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>none</Arguments>
            <Returns>BibID and VID</Returns>
          </AutoDoc>
        </Endpoint>
        <Endpoint Segment="rdf" ComponentID="C3" Method="GetItemRdf" Protocol="XML">
          <AutoDoc>
            <Description>Get the item information, as dublin core based RDF</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>BibID and VID</Arguments>
            <Returns>RDF-formatted XML</Returns>
          </AutoDoc>
        </Endpoint>
        <Endpoint Segment="xml" ComponentID="C3" Method="GetItemXml" Protocol="XML">
          <AutoDoc>
            <Description>Get the item information formatted in a number of different XML schemas</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Format and BibID and VID</Arguments>
            <Returns>XML output of a version/format of the item informaiton</Returns>
          </AutoDoc>
        </Endpoint>
      </Path>
      <Path Segment="search">
        <Path Segment="legacy">
          <Endpoint Segment="json" ComponentID="C6" Method="Get_Search_Results_Legacy" Protocol="JSON">
            <AutoDoc>
              <Description>Gets the search results and returns them in the legacy format supported prior to v5.0</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments></Arguments>
              <Returns>Legacy formatted JSON response</Returns>
            </AutoDoc>
          </Endpoint>
          <Endpoint Segment="xml" ComponentID="C6" Method="Get_Search_Results_Legacy" Protocol="XML">
            <AutoDoc>
              <Description>Gets the search results and returns them in the legacy format supported prior to v5.0</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments></Arguments>
              <Returns>Legacy formatted XML response</Returns>
            </AutoDoc>
          </Endpoint>
        </Path>
        <Path Segment="results">
          <AutoDoc>
            <Description>Gets a page of search results</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Aggrgeation code</Arguments>
            <Returns>Search_Results_Statistics</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C6" Method="Get_Search_Results_Page" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C6" Method="Get_Search_Results_Page" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C6" Method="Get_Search_Results_Page" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C6" Method="Get_Search_Results_Page" Protocol="XML" />
        </Path>
        <Path Segment="stats">
          <AutoDoc>
            <Description>Gets statistics for a results set </Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Aggregation code and all the search info in the query string</Arguments>
            <Returns>Search_Results_Statistics</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C6" Method="Get_Search_Statistics" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C6" Method="Get_Search_Statistics" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C6" Method="Get_Search_Statistics" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C6" Method="Get_Search_Statistics" Protocol="XML" />
        </Path>
      </Path>
      <Path Segment="skins">
        <Path Segment="all">
          <AutoDoc>
            <Description>Gets the ordered list of all web skins in the system</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>none</Arguments>
            <Returns>List&lt;string&gt;</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C2" Method="GetOrderedCodes" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C2" Method="GetOrderedCodes" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C2" Method="GetOrderedCodes" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C2" Method="GetOrderedCodes" Protocol="XML" />
        </Path>
        <Path Segment="code">
          <AutoDoc>
            <Description>Gets a single web skin object, fully built and language specific</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Web skin code and language</Arguments>
            <Returns>SobekCM.Core.Skins.Web_Skin_Object</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C2" Method="GetWebSkin" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C2" Method="GetWebSkin" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C2" Method="GetWebSkin" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C2" Method="GetWebSkin" Protocol="XML" />
        </Path>
        <Path Segment="complete">
          <AutoDoc>
            <Description>Gets a single web skin object</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Web skin code</Arguments>
            <Returns>SobekCM.Core.Skins.Complete_Web_Skin_Object</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C2" Method="GetCompleteWebSkin" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C2" Method="GetCompleteWebSkin" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C2" Method="GetCompleteWebSkin" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C2" Method="GetCompleteWebSkin" Protocol="XML" />
        </Path>
        <Endpoint Segment="uploads" ComponentID="C2" Method="GetWebSkinUploadedImages" Protocol="JSON">
          <AutoDoc>
            <Description>Gets the list of uploaded images for this web skin </Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Web skin code</Arguments>
            <Returns>List&lt;SobekCM.Engine_Library.JSON_Client_Helpers.UploadedFileFolderInfo&gt;</Returns>
          </AutoDoc>
        </Endpoint>
      </Path>
      <Path Segment="url-resolver">
        <AutoDoc>
          <Description>Resolves the url to a SobekCM Navigation_Object</Description>
          <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
          <Arguments>many possible</Arguments>
          <Returns>SobekCM.Core.Navigation.Navigation_Object</Returns>
        </AutoDoc>
        <Endpoint Segment="json" ComponentID="C4" Method="ResolveUrl" Protocol="JSON" />
        <Endpoint Segment="json-p" ComponentID="C4" Method="ResolveUrl" Protocol="JSON-P" />
        <Endpoint Segment="protobuf" ComponentID="C4" Method="ResolveUrl" Protocol="PROTOBUF" />
        <Endpoint Segment="xml" ComponentID="C4" Method="ResolveUrl" Protocol="XML" />
      </Path>
      <Path Segment="webcontent">
        <Path Segment="all">
          <Path Segment="has">
            <AutoDoc>
              <Description>Returns a flag indicating if there are any web content entities, including pages and redirects</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments>(none)</Arguments>
              <Returns>TRUE or FALSE</Returns>
            </AutoDoc>
            <Endpoint Segment="json" ComponentID="C5" Method="Has_Pages_Or_Redirects" Protocol="JSON" />
            <Endpoint Segment="json-p" ComponentID="C5" Method="Has_Pages_Or_Redirects" Protocol="JSON-P" />
            <Endpoint Segment="protobuf" ComponentID="C5" Method="Has_Pages_Or_Redirects" Protocol="PROTOBUF" />
            <Endpoint Segment="xml" ComponentID="C5" Method="Has_Pages_Or_Redirects" Protocol="XML" />
          </Path>
          <Path Segment="list">
            <AutoDoc>
              <Description>Get the list of all the web content entities, including pages and redirects</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments>Page number in the URL, rowsPerPage.. all optional </Arguments>
              <Returns>WebContent_Basic_Pages</Returns>
            </AutoDoc>
            <Endpoint Segment="json" ComponentID="C5" Method="Get_All" Protocol="JSON" />
            <Endpoint Segment="json-p" ComponentID="C5" Method="Get_All" Protocol="JSON-P" />
            <Endpoint Segment="protobuf" ComponentID="C5" Method="Get_All" Protocol="PROTOBUF" />
            <Endpoint Segment="xml" ComponentID="C5" Method="Get_All" Protocol="XML" />
            <Endpoint Segment="jtable" ComponentID="C5" Method="Get_All_JDataTable" Protocol="JSON">
              <AutoDoc>
                <Description> Get the list of all the web content entities, including pages and redirects for consumption by the jQuery DataTable.net plug-in </Description>
                <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
                <Arguments>Page number in the URL, rowsPerPage.. all optional </Arguments>
                <Returns>JSON response configured for the jQuery DataTables.net plugin</Returns>
              </AutoDoc>
            </Endpoint>
          </Path>
          <Path Segment="next-level">
            <AutoDoc>
              <Description>Gets the list of possible next level from an existing point in the hierarchy</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments>Levels within the queried URL, just like requesting a page</Arguments>
              <Returns>List&lt;string&gt;</Returns>
            </AutoDoc>
            <Endpoint Segment="json" ComponentID="C5" Method="Get_All_NextLevel" Protocol="JSON" />
            <Endpoint Segment="json-p" ComponentID="C5" Method="Get_All_NextLevel" Protocol="JSON-P" />          
            <Endpoint Segment="protobuf" ComponentID="C5" Method="Get_All_NextLevel" Protocol="PROTOBUF" />
            <Endpoint Segment="xml" ComponentID="C5" Method="Get_All_NextLevel" Protocol="XML" />
          </Path>
        </Path>
        <Path Segment="css">
          <Path Segment="all">
            <AutoDoc>
              <Description>Gets the list of all controlled CSS stylesheets in the system</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments>None</Arguments>
              <Returns>List&lt;string&gt;</Returns>
            </AutoDoc>
            <Endpoint Segment="json" ComponentID="C5" Method="Get_All_Controlled_Stylesheets" Protocol="JSON" />
            <Endpoint Segment="json-p" ComponentID="C5" Method="Get_All_Controlled_Stylesheets" Protocol="JSON-P" />
            <Endpoint Segment="protobuf" ComponentID="C5" Method="Get_All_Controlled_Stylesheets" Protocol="PROTOBUF" />
            <Endpoint Segment="xml" ComponentID="C5" Method="Get_All_Controlled_Stylesheets" Protocol="XML" />
          </Path>
        </Path>
        <Path Segment="hierarchy">
          <AutoDoc>
            <Description>Get the complete hierarchy of web content pages and redirects, used for navigation</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>(none)</Arguments>
            <Returns>Tree structure of the hierarchy of all the web conten pages and redirects</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C5" Method="Get_Hierarchy" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C5" Method="Get_Hierarchy" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C5" Method="Get_Hierarchy" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C5" Method="Get_Hierarchy" Protocol="XML" />
        </Path>
        <Path Segment="hits">
          <AutoDoc>
            <Description>Get the complete monthly usage for a single web content page</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Primary key to the web content page in the URL</Arguments>
            <Returns>Usage report for a single web page ( Single_WebContent_Usage_Report) </Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C5" Method="Get_Single_Usage_Report" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C5" Method="Get_Single_Usage_Report" Protocol="JSON-P" />
          <Endpoint Segment="protobuf" ComponentID="C5" Method="Get_Single_Usage_Report" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C5" Method="Get_Single_Usage_Report" Protocol="XML" />
        </Path>
        <Path Segment="javascript">
          <Path Segment="all">
            <AutoDoc>
              <Description>Gets the list of all controlled javascript files in the system</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments>None</Arguments>
              <Returns>List&lt;string&gt;</Returns>
            </AutoDoc>
            <Endpoint Segment="json" ComponentID="C5" Method="Get_All_Controlled_Javascript" Protocol="JSON" />
            <Endpoint Segment="json-p" ComponentID="C5" Method="Get_All_Controlled_Javascript" Protocol="JSON-P" />
            <Endpoint Segment="protobuf" ComponentID="C5" Method="Get_All_Controlled_Javascript" Protocol="PROTOBUF" />
            <Endpoint Segment="xml" ComponentID="C5" Method="Get_All_Controlled_Javascript" Protocol="XML" />
          </Path>
        </Path>
        <Path Segment="milestones">
          <AutoDoc>
            <Description>Get the list of milestones affecting a single (non aggregation affiliated) static web content page</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>Primary key to the web content page in the URL</Arguments>
            <Returns>Milestone report for a single web page ( Single_WebContent_Change_Report )</Returns>
          </AutoDoc>
          <ComplexEndpoint Segment="json">
            <VerbMapping Verb="GET" ComponentID="C5" Method="Get_Single_Milestones" Protocol="JSON" />
            <VerbMapping Verb="POST" ComponentID="C5" Method="Add_Milestone" Protocol="JSON" />
          </ComplexEndpoint>
          <ComplexEndpoint Segment="json-p">
            <VerbMapping Verb="GET" ComponentID="C5" Method="Get_Single_Milestones" Protocol="JSON-P" />
          </ComplexEndpoint>
          <ComplexEndpoint Segment="protobuf">
            <VerbMapping Verb="GET" ComponentID="C5" Method="Get_Single_Milestones" Protocol="PROTOBUF" />
            <VerbMapping Verb="POST" ComponentID="C5" Method="Add_Milestone" Protocol="PROTOBUF" />
          </ComplexEndpoint>
          <ComplexEndpoint Segment="xml">
            <VerbMapping Verb="GET" ComponentID="C5" Method="Get_Single_Milestones" Protocol="XML" />
            <VerbMapping Verb="POST" ComponentID="C5" Method="Add_Milestone" Protocol="XML" />
          </ComplexEndpoint>
        </Path>
        <Path Segment="missing">
          <AutoDoc>
            <Description>Gets the special missing web content page, used when a requested resource is missing</Description>
            <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
            <Arguments>None</Arguments>
            <Returns>SobekCM.Core.Navigation.HTML_Based_Content</Returns>
          </AutoDoc>
          <Endpoint Segment="json" ComponentID="C5" Method="Get_Special_Missing_Page" Protocol="JSON" />
          <Endpoint Segment="json-p" ComponentID="C5" Method="Get_Special_Missing_Page" Protocol="JSON-P" />         
          <Endpoint Segment="protobuf" ComponentID="C5" Method="Get_Special_Missing_Page" Protocol="PROTOBUF" />
          <Endpoint Segment="xml" ComponentID="C5" Method="Get_Special_Missing_Page" Protocol="XML" />
        </Path>
        <Path Segment="pages">
          <Path Segment="has">
            <AutoDoc>
              <Description>Returns a flag indicating if there are any web content pages (excluding redirects)</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments>(none)</Arguments>
              <Returns>TRUE or FALSE</Returns>
            </AutoDoc>
            <Endpoint Segment="json" ComponentID="C5" Method="Has_Content_Pages" Protocol="JSON" />
            <Endpoint Segment="json-p" ComponentID="C5" Method="Has_Content_Pages" Protocol="JSON-P" />
            <Endpoint Segment="protobuf" ComponentID="C5" Method="Has_Content_Pages" Protocol="PROTOBUF" />
            <Endpoint Segment="xml" ComponentID="C5" Method="Has_Content_Pages" Protocol="XML" />
          </Path>
          <Path Segment="list">
            <AutoDoc>
              <Description>Get the list of all the web content pages ( excluding redirects )</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments>Page number in the URL, rowsPerPage.. all optional </Arguments>
              <Returns>WebContent_Basic_Pages</Returns>
            </AutoDoc>
            <Endpoint Segment="json" ComponentID="C5" Method="Get_All_Pages" Protocol="JSON" />
            <Endpoint Segment="json-p" ComponentID="C5" Method="Get_All_Pages" Protocol="JSON-P" />
            <Endpoint Segment="protobuf" ComponentID="C5" Method="Get_All_Pages" Protocol="PROTOBUF" />
            <Endpoint Segment="xml" ComponentID="C5" Method="Get_All_Pages" Protocol="XML" />
            <Endpoint Segment="jtable" ComponentID="C5" Method="Get_All_Pages_JDataTable" Protocol="JSON" >
              <AutoDoc>
                <Description> Get the list of all the web content pages ( excluding redirects ) for consumption by the jQuery DataTable.net plug-in </Description>
                <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
                <Arguments>Page number in the URL, rowsPerPage.. all optional </Arguments>
                <Returns>JSON response configured for the jQuery DataTables.net plugin</Returns>
              </AutoDoc>
            </Endpoint>
          </Path>
          <Path Segment="next-level">
            <AutoDoc>
              <Description>Gets the list of possible next level from an existing point in the hierarchy</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments>Levels within the queried URL, just like requesting a page</Arguments>
              <Returns>List&lt;string&gt;</Returns>
            </AutoDoc>
            <Endpoint Segment="json" ComponentID="C5" Method="Get_All_Pages_NextLevel" Protocol="JSON" />
            <Endpoint Segment="json-p" ComponentID="C5" Method="Get_All_Pages_NextLevel" Protocol="JSON-P" />
            <Endpoint Segment="protobuf" ComponentID="C5" Method="Get_All_Pages_NextLevel" Protocol="PROTOBUF" />
            <Endpoint Segment="xml" ComponentID="C5" Method="Get_All_Pages_NextLevel" Protocol="XML" />
          </Path>
        </Path>
        <Path Segment="recent">
          <Path Segment="has">
            <AutoDoc>
              <Description>Returns a flag indicating if there are any global recent updates to the web content entities (pages and redirects)</Description>
              <RequestType>This endpoint does not support updates or deletes and only the GET HTTP verb is supported</RequestType>
              <Arguments>(none)</Arguments>
              <Returns>TRUE or FALSE</Returns>
            </AutoDoc>
            <Endpoint Segment="json" ComponentID="C5" Method="Has_Global_Recent_Updates" Protocol="JSON" />
            <Endpoint Segment="json-p" ComponentID="C5" Method="Has_Global_Recent_Updates" Protocol="JSON-P" />
            <Endpoint Segment="protobuf" ComponentID="C5" Method="Has_Global_Recent_Updates" Protocol="PROTOBUF" />
            <Endpoint Segment="xml" ComponentID="C5" Method="Has_Global_Recent_Updates" Protocol="XML" />
          </Path>
